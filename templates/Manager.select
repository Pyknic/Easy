global $db;

if (!isset($this->cache[$selector])) {
	$this->cache[$selector] = array();
	
	$result = $db->query(
		"SELECT %0 " .
		"FROM %1 " .
		"WHERE $selector" .
		($order === null) ? ";" : 
		" ORDER BY $order;"
	) or die ($db->error);

	while ($row = $result->fetch_assoc()) {
		$this->cache[$selector][] = %2::fromRow($row);
	}
}

return $this->cache[$selector];